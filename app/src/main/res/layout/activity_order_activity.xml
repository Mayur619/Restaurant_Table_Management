<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back">

    <TextView
        android:id="@+id/Menu"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignParentEnd="true"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:text="TextView" />

    <TextView
        android:id="@+id/table"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignParentEnd="true"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:text="TextView" />

    <TextView
        android:id="@+id/user_con"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignParentEnd="true"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:text="TextView" />

    <TextView
        android:id="@+id/date_time"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:text="TextView" />

    <TextView
        android:id="@+id/res_add"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignParentEnd="true"
        android:text="TextView" />

    <TextView
        android:id="@+id/user_name"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView5"
        android:text="TextView"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"/>

    <TextView
        android:id="@+id/res_name"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/user_name"
        android:text="TextView"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"/>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="22dp"
        android:text="Cost"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAlignment="viewStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/cui"
        android:layout_below="@+id/cui"
        android:layout_marginTop="25dp"
        android:text="Table No."
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAlignment="viewStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="29dp"
        android:text="User Contact No."
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4"
        android:layout_marginStart="12dp"
        android:layout_marginTop="17dp"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:text="Restaurant"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"

        />

    <TextView
        android:id="@+id/cui"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:text="User Contact No."
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView12"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="22dp"
        android:text="Date and time"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/textView8"
        android:fontFamily="cursive"
        android:text="Order Details"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="36sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="19dp"
        android:text="Name of User"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="23dp"
        android:text="Menu"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAlignment="viewStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="17dp"
        android:text="Address of Restaurant"
        android:textAlignment="viewStart"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/cost"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignParentEnd="true"
        android:text="TextView"
        android:fontFamily="cursive"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="30sp"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cost"
        android:layout_centerHorizontal="true"
        android:text="Confirm"
        android:background="@drawable/nextstyle"/>

</RelativeLayout>
